{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

    "data": {
       "url": "https://raw.githubusercontent.com/Jamesvh12346/Jamesvh12346.github.io/main/portfolio%20json/SOLcoinapi.json"
       },


    "title": {
        "text": "Price of Solana",
        "subtitle": "10 day price average. Source: CoinAPI",
        "subtitleFontStyle":"italic",
        "subtitleFontSize":10,
        "anchor": "start",
        "color": "black"},

    "width":435,
    "height":300,

    "mark":{
        "type": "line",
        "color":"orange",
        "strokeWidth":2,
        "opacity":1},
 

    "encoding": {
      "x": {
          "field": "time_period_end",
          "type": "temporal",
          "title":null},
      "y": {
          "field": "rate_close",
          "type": "quantitative",
          "title":null}
    }
}
