{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Covid Hospital Beds (added false observation)",
    "anchor": "start",
    "color": "black"
  },
  "height": 300,
  "width": 280,
  "data": {
    "url": "https://raw.githubusercontent.com/Jamesvh12346/Jamesvh12346.github.io/main/hospitalbeds.csv",
    "format": {"type": "csv"}
  },
  "mark": {
    "type": "line",
    "interpolate": "monotone",
    "color": "mediumvioletred"
  },
  "encoding": {
    "x": {"field": "date", "type": "temporal", "axis": {"labelAngle": 0, "grid": true}, "title": "Date"},
    "y": {"field": "covidOccupiedMVBeds", "type": "quantitative", "axis": {"grid": true}, "title": "CovidHospitalBeds"}
  }
}
